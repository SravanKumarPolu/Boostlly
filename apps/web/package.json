{
  "name": "@boostlly/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/update-version.js",
    "build": "npm run clean && npm run prebuild && NODE_ENV=production next build && npm run postbuild",
    "postbuild": "node scripts/update-version.js",
    "build:web": "npm run clean && npm run prebuild && NODE_ENV=production next build && npm run postbuild",
    "preview:web": "next start -p 3000",
    "analyze:web": "npm run clean && ANALYZE=true NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:web": "next lint --dir src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist",
    "update-version": "node scripts/update-version.js"
  },
  "dependencies": {
    "@boostlly/core": "workspace:*",
    "@boostlly/features": "workspace:*",
    "@boostlly/platform-web": "workspace:*",
    "@boostlly/ui": "workspace:*",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "fuse.js": "^7.1.0",
    "howler": "^2.2.4",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.32",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.0"
  }
}
